<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.eclipse.org/emf/diffmerge/diffdata/1.0.0"
            class="org.eclipse.emf.diffmerge.diffdata.DiffdataPackage"
            genModel="model/DiffData.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <!-- @generated DiffData -->
      <parser
            type="diffdata"
            class="org.eclipse.emf.diffmerge.diffdata.util.DiffdataResourceFactoryImpl"/>
   </extension>

</plugin>
